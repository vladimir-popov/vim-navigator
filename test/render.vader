Given(Dirty buffer):
  A previously rendered content

Execute(Sections):
  let sections = [
      \ { 'begin': 1, 'end': 4, 'fold': 1, 'title': 'Section 1' },
      \ { 'begin': 2, 'end': 3, 'fold': 2, 'title': 'Section 1.1' },
      \ { 'begin': 5, 'end': 6, 'fold': 1, 'title': 'Section 2' },
      \ ]

Execute(Render a simple contents):
  let render = navigator#render#New()
  let rendered = render.renderAll(sections)
  call append('$', '----')  

  " number of line in the Contents buffer must be added 
  " to rendered sections
  for i in range(len(sections))
    let id = sections[i].begin
    AssertEqual i + 1, rendered[id].line
  endfor

Expect(Contents):
    Section 1
      Section 1.1
    Section 2
  ----
